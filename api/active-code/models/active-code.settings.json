    "department": {
      "type": "string"
    },
    "activeDate": {
      "type": "datetime"
    },
    "status": {
      "type": "string"
    },
    "cv": {
      "model": "cv",
      "via": "activeCode"
    },
    "userPhone": {
      "type": "string"
    }
  }
}
